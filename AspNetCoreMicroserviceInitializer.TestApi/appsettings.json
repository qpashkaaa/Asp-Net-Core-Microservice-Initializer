{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "TestTaskSettings": {
    "CronExpression": "*/1 * * * *",
    "TimeZone": "Europe/Moscow"
  },
  "HangfireDashboardSettings": {
    "EnableCustomAuthorization": false,
    "FilterName": null
  },
  "HangfireSettings": {
    "StorageType": 2,
    "PostgreSqlConnectionString": null
  },
  "HealthChecksSettings": {
    "Endpoint": "/health",
    "UIEnable": true,
    "UIEvaluationTimeInSeconds": 5,
    "UIApiMaxActiveRequests": 2,
    "EndpointFullUrl": "http://localhost:44027/health"
  },
  "TestSetting": {
    "Message": "Edited from config",
    "Flag": true
  },
  "MyDbContextSettings": {
    "ConnectionString": "Host=localhost:5432; Database=postgres; Username=postgres; Password=admin",
    "Schema": "myschema",
    "MigrationsTableName": "__EFMigrationsHistory",
    "MigrationsSchema": "myschema"
  },
  "TestDbContextSettings": {
    "ConnectionString": "Host=localhost:5555; Database=postgres; Username=postgres; Password=postgres",
    "Schema": "MySchema",
    "MigrationsTableName": "__EFMigrationsHistory",
    "MigrationsSchema": "MySchema"
  },
  "CorsSettings": {
    "EnableCors": true,
    "PolicyName": "AllowAccessFrontendQueries",
    "AllowedOrigins": []
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.SQLite"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "HealthChecks": "Warning",
        "AspNetCore.HealthChecks.UI": "Warning",
        "AspNetCore.HealthChecks.UI.Client": "Warning",
        "AspNetCore.HealthChecks.UI.InMemory.Storage": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "OutputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Exception}",
        "Args": null
      },
      {
        "Name": "SQLite",
        "OutputTemplate": null,
        "Args": {
          "SqliteDbPath": "logs/logs.db",
          "TableName": "Logs"
        }
      }
    ],
    "Properties": {
      "ApplicationName": "ApplicationName"
    }
  },
  "EnvironmentVariablesSettingTest": {
    "Message": "Hi from appsettings.json",
    "OverwriteMessageByEnv": null
  }
}